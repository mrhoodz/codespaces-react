#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules/@builder.io/qwik/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules/@builder.io/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules/@builder.io/qwik/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules/@builder.io/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/@builder.io+qwik@1.5.1_@types+node@20.11.25_undici@6.7.1/node_modules:/mnt/c/mac/kathukonnect-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../qwik-cli.cjs" "$@"
else
  exec node  "$basedir/../../qwik-cli.cjs" "$@"
fi
